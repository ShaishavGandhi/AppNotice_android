    <string name="ghostery_preferences_description">Para más información sobre las tecnologías usadas en esta aplicación, consulte la lista que se muestra a continuación.</string>
    <!-- Text of the title of optional trackers tab. -->
    <string name="ghostery_preferences_optional_title">Opcional</string>
    <!-- Text of the title of essential trackers tab. -->
    <string name="ghostery_preferences_essential_title">Esencial</string>
    <!-- Text of the title of web-based trackers tab. -->
    <string name="ghostery_preferences_webbased_title">Vía web</string>
    <!-- Text of the message above the list of optional trackers. -->
    <string name="ghostery_preferences_optional_message">Las siguientes tecnologías opcionales pueden desactivarse si lo desea.</string>
    <!-- Text of the message above the list of essential trackers. -->
    <string name="ghostery_preferences_essential_message">Las siguientes tecnologías son necesarias para que la aplicación funcione y se enumeran solo a título informativo.</string>
    <!-- Text of the message above the list of web-based trackers. -->
    <string name="ghostery_preferences_webbased_message">Algunas partes de esta aplicación usan vistas web que pueden usar cookies.  Puede desactivar cookies ulteriormente, en configuraciones de la aplicación.</string>
    <!-- Text of the privacy tools message at the bottom of the privacy preferences screen. -->
    <string name="ghostery_preferences_controls_message">Controles de privacidad, Ghostery, Inc.</string>
    <!-- Text of the message on the implied manage preferences snackbar. -->
    <string name="ghostery_preferences_ready_message">Empezar</string>
    <!-- Text of the action on the implied manage preferences snackbar. -->
    <string name="ghostery_preferences_continue_button">Continuar</string>
